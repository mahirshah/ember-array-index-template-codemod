{
  "name": "ember-array-index-template-codemod",
  "version": "1.0.0",
  "description": "Codemods for transforming template array access to use get helper where appropriate",
  "homepage": "https://github.com/mahirshah/ember-array-index-template-codemod#readme",
  "bugs": {
    "url": "https://github.com/mahirshah/ember-array-index-template-codemod/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mahirshah/ember-array-index-template-codemod.git"
  },
  "scripts": {
    "lint": "eslint --cache .",
    "test": "codemod-cli test",
    "test:coverage": "codemod-cli test --coverage",
    "update-docs": "codemod-cli update-docs",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "release": "release-it"
  },
  "bin": "./bin/cli.js",
  "keywords": [
    "codemod-cli"
  ],
  "dependencies": {
    "codemod-cli": "^2.1.0",
    "ember-template-recast": "^4.0.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.6",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "release-it": "^12.4.3"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "jest": {
    "testEnvironment": "node"
  }
